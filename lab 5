#include <stdio.h>

const int MOD = 12345;  // Константа для обчислення залишку

// Функція для підрахунку кількості послідовностей
int countSequences(int n) {
    int dp[n+1];  // Масив для зберігання проміжних результатів
    dp[0] = 1;  // Базовий випадок: 1 послідовність з нуля елементів
    dp[1] = 2;  // Базовий випадок: 2 послідовності з одного елемента
    dp[2] = 4;  // Базовий випадок: 4 послідовності з двох елементів

    // Заповнення масиву з третього елемента до n
    for (int i = 3; i <= n; i++) {
        // Обчислення кількості послідовностей для поточного i
        // як суми трьох попередніх значень з масиву dp
        dp[i] = (dp[i-1] + dp[i-2] + dp[i-3]) % MOD;
    }

    return dp[n];  // Повертаємо кількість шуканих послідовностей
}

int main() {
    int n;
    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

    int result = countSequences(n);
    printf("Кількість шуканих послідовностей: %d\n", result);

    return 0;
}
